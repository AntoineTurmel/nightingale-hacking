<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/">
<ShortName>Dogpile</ShortName>
<Description>Dogpile audio search</Description>
<InputEncoding>UTF-8</InputEncoding>
<Image width="16" height="16">chrome://service-icons/skin/dogpile.ico</Image>
<Url type="text/html" method="GET" template="http://www.dogpile.com/info.dogpl/search/redir.htm">
  <Param name="q_all" value="{searchTerms}"/>      
  <Param name="q_phrase" value=""/>
  <Param name="q_not" value=""/>  
  <Param name="r_fcid" value="414"/>
  <Param name="r_fcp" value="top"/>
  <Param name="advanced" value="1"/>
  <Param name="top" value="1"/>
  <Param name="nde" value="1"/>
  <Param name="qcat" value="audio"/>
  <Param name="duration" value="long"/>
  <Param name="ffmt" value="mp3"/>
  <Param name="tviewby" value="1"/>
  <Param name="qk" value="40"/>
  <Param name="adultfilter" value="none"/>      
  <Param name="bottomadvancedsubmit" value="Go+Fetch%21"/>
</Url>
<SearchForm>http://www.dogpile.com/</SearchForm>
</SearchPlugin>
