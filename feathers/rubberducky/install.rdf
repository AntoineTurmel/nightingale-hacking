<?xml version="1.0"?>

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">

  <Description about="urn:mozilla:install-manifest">
    <em:id>rubberducky@songbirdnest.com</em:id>
    <em:version>1.0</em:version>

    <!-- Songbird -->
    <em:targetApplication>
      <Description>
        <em:id>songbird@songbirdnest.com</em:id>
        <em:minVersion>0.2</em:minVersion>
        <em:maxVersion>0.2</em:maxVersion>
      </Description>
    </em:targetApplication>

    <em:name>Songbird Rubberducky (default)</em:name>
    <em:description>Default Songbird theme</em:description>
    <em:locked>true</em:locked>
    <em:creator>Pioneers of the Inevitable</em:creator>

    <!-- internalName sets "skin" mode for the extension and prevents
         declaration of content or overlays. Turning it off for now so that we
         can put the default bones in with rubberducky. Also adding the
         <em:hidden> tag to keep it from showing up in the EM. -->

    <!-- <em:internalName>rubberducky/0.2</em:internalName> -->
    <em:hidden>true</em:hidden>
    <em:appManaged>true</em:appManaged>
  </Description>

</RDF>
