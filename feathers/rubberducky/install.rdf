<?xml version="1.0"?>

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#"
     xmlns:songbird="http://www.songbirdnest.com/2007/addon-metadata-rdf#">

  <Description about="urn:mozilla:install-manifest">
    <em:id>rubberducky@songbirdnest.com</em:id>
    <em:version>1.0</em:version>

    <!-- Songbird -->
    <em:targetApplication>
      <Description>
        <em:id>songbird@songbirdnest.com</em:id>
        <em:minVersion>0.2</em:minVersion>
        <em:maxVersion>0.3</em:maxVersion>
      </Description>
    </em:targetApplication>

    <em:name>Songbird Rubberducky (default)</em:name>
    <em:description>Default Songbird theme</em:description>
    <em:locked>true</em:locked>
    <em:creator>Pioneers of the Inevitable</em:creator>

    <songbird:feathers>
      <Description>
        <songbird:skin>
          <Description>
            <songbird:name>Rubberducky (default)</songbird:name>
            <songbird:internalName>rubberducky/0.2</songbird:internalName>
            <songbird:compatibleLayout>
              <Description>
                <songbird:layoutURL>chrome://rubberducky/content/xul/mainwin.xul</songbird:layoutURL>
                <songbird:showChrome>false</songbird:showChrome>
              </Description>
            </songbird:compatibleLayout>
          </Description>
        </songbird:skin>
        <songbird:layout>
          <Description>
            <songbird:name>Standard Player</songbird:name>
            <songbird:url>chrome://rubberducky/content/xul/mainwin.xul</songbird:url>
            <songbird:compatibleSkin>
              <Description>
                <songbird:internalName>rubberducky/0.2</songbird:internalName>
                <songbird:showChrome>false</songbird:showChrome>
              </Description>
            </songbird:compatibleSkin>
          </Description>
        </songbird:layout>
        <songbird:layout>
          <Description>
            <songbird:name>Mini Player</songbird:name>
            <songbird:url>chrome://rubberducky/content/xul/miniplayer.xul</songbird:url>
            <songbird:compatibleSkin>
              <Description>
                <songbird:internalName>rubberducky/0.2</songbird:internalName>
                <songbird:showChrome>false</songbird:showChrome>
              </Description>
            </songbird:compatibleSkin>
          </Description>
        </songbird:layout>        
      </Description>
    </songbird:feathers>

    <em:hidden>true</em:hidden>
    <em:appManaged>true</em:appManaged>
  </Description>

</RDF>
