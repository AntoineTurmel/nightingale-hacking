#filter substitution
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#"
     xmlns:songbird="http://www.songbirdnest.com/2007/addon-metadata-rdf#">

  <Description about="urn:mozilla:install-manifest">

    <!-- Songbird target application. -->
    <em:targetApplication>
      <Description>
        <em:id>songbird@songbirdnest.com</em:id>
        <em:minVersion>@EXTENSION_MIN_VER@</em:minVersion>
        <em:maxVersion>@EXTENSION_MAX_VER@</em:maxVersion>
      </Description>
    </em:targetApplication>

    <em:id>shoutcast-radio@songbirdnest.com</em:id>
    <em:name>SHOUTcast Radio</em:name>
    <em:description>Directory of SHOUTcast Internet radio streams</em:description>
    <em:creator>Stephen Lau (stevel@songbirdnest.com)</em:creator>
    <em:version>@EXTENSION_VER@</em:version>
    <em:iconURL>chrome://shoutcast-radio/skin/shoutcast_icon-32x32.png</em:iconURL>
	<em:optionsURL>chrome://shoutcast-radio/content/optionsWindow.xul</em:optionsURL>
	<em:homepageURL>http://shoutcast.com</em:homepageURL>

    <em:type>2</em:type>
  </Description>
</RDF>

