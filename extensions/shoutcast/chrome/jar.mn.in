% locale shoutcast-radio ru-RU %locale/ru/
% locale shoutcast-radio sl-SI %locale/sl-SI/
#% locale shoutcast-radio sr-RS %locale/sr/
#% locale shoutcast-radio tr-TR %locale/tr/
