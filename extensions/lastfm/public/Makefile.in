DEPTH     = ../../..
topsrcdir = @top_srcdir@
srcdir    = @srcdir@
VPATH     = @srcdir@

include $(DEPTH)/build/autodefs.mk

XPIDL_SRCS = sbILastFm.idl \
             $(NULL)

XPIDL_MODULE = sbLastFm.xpt
XPIDL_MODULE_TYPELIBS = $(XPIDL_SRCS:.idl=.xpt)

include $(topsrcdir)/build/rules.mk

TARGET_DIR = ../.xpistage/components

include $(topsrcdir)/build/rules.mk

all::
	$(MKDIR) -p $(TARGET_DIR)
	$(CP) -f $(XPIDL_MODULE) $(TARGET_DIR)
