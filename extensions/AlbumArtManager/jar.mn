albumartmanager.jar:
% content albumartmanager %content/albumartmanager/
% skin albumartmanager classic/1.0 %skin/albumartmanager/
% locale albumartmanager en-US %locale/albumartmanager/en-US/
% overlay chrome://songbird/content/xul/layoutBaseOverlay.xul chrome://albumartmanager/content/overlay/overlayMenu.xul
% overlay chrome://browser/content/preferences/preferences.xul chrome://albumartmanager/content/overlay/overlayPrefs.xul
% override chrome://global/content/alerts/alert.xul chrome://albumartmanager/content/xul/alert.xul
% override chrome://global/content/alerts/alert.js chrome://albumartmanager/content/scripts/alert.js
% override chrome://global/skin/alerts/alert.css chrome://albumartmanager/skin/alert.css
  content/albumartmanager/bindings/albumart.xml             (chrome/content/bindings/albumart.xml)
  content/albumartmanager/overlay/overlayMenu.xul           (chrome/content/overlay/overlayMenu.xul)
  content/albumartmanager/overlay/overlayPrefs.xul          (chrome/content/overlay/overlayPrefs.xul)
  content/albumartmanager/scripts/albumartmanager.js        (chrome/content/scripts/albumartmanager.js)
  content/albumartmanager/scripts/alert.js                  (chrome/content/scripts/alert.js)
  content/albumartmanager/scripts/overlayMenu.js            (chrome/content/scripts/overlayMenu.js)
  content/albumartmanager/scripts/prefPane.js               (chrome/content/scripts/prefPane.js)
  content/albumartmanager/xul/albumartmanager.xul           (chrome/content/xul/albumartmanager.xul)
  content/albumartmanager/xul/coverPreview.xul              (chrome/content/xul/coverPreview.xul)
  content/albumartmanager/xul/alert.xul                     (chrome/content/xul/alert.xul)
  content/albumartmanager/xul/openPrefs.xul                 (chrome/content/xul/openPrefs.xul)
  content/albumartmanager/xul/fetcherPriorities.xul         (chrome/content/xul/fetcherPriorities.xul)
  content/albumartmanager/xul/servicePane.xul               (chrome/content/xul/servicePane.xul)
  locale/albumartmanager/en-US/albumartmanager.dtd          (chrome/locale/en-US/albumartmanager.dtd)
  locale/albumartmanager/en-US/albumartmanager.properties   (chrome/locale/en-US/albumartmanager.properties)
  skin/albumartmanager/albumart.css                         (chrome/skin/albumart.css)
  skin/albumartmanager/alert.css                            (chrome/skin/alert.css)
  skin/albumartmanager/PrefsIcon.png                        (chrome/skin/PrefsIcon.png)
  skin/albumartmanager/no-cover.png                         (chrome/skin/no-cover.png)
