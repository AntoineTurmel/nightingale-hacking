#filter substitution
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#"
     xmlns:songbird="http://www.songbirdnest.com/2007/addon-metadata-rdf#">

  <Description about="urn:mozilla:install-manifest">

    <!-- Songbird target application. -->
    <em:targetApplication>
      <Description>
        <em:id>songbird@songbirdnest.com</em:id>
        <em:minVersion>@EXTENSION_MIN_VER@</em:minVersion>
        <em:maxVersion>@EXTENSION_MAX_VER@</em:maxVersion>
      </Description>
    </em:targetApplication>

    <em:id>albumartmanager@songbirdnest.com</em:id>
    <em:name>Album Art Manager</em:name>
    <em:description>Album Art Manager for Songbird.</em:description>
    <em:creator>Steven Bengtson</em:creator>
    <em:version>@EXTENSION_VER@</em:version>
    <em:iconURL>chrome://albumartmanager/skin/PrefsIcon.png</em:iconURL>
    <em:optionsURL>chrome://albumartmanager/content/xul/openPrefs.xul</em:optionsURL>

    <em:type>2</em:type>
  </Description>
</RDF>

