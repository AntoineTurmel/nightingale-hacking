mashTape.jar:
% content mashtape %content/
% resource mashtape modules/
% locale mashtape en-US %locale/en-US/
% locale mashtape en-GB %locale/en-GB/
% locale mashtape bn-IN %locale/bn-IN/
% locale mashtape zh-CN %locale/zh-CN/
% locale mashtape cs-CZ %locale/cs-CZ/
% locale mashtape nl-NL %locale/nl-NL/
% locale mashtape fr-FR %locale/fr-FR/
% locale mashtape it-IT %locale/it-IT/
% locale mashtape ja-JP %locale/ja-JP/
% locale mashtape pl-PL %locale/pl-PL/
% locale mashtape pt-BR %locale/pt-BR/
% locale mashtape es-ES %locale/es-ES/
% locale mashtape sv-SE %locale/bn-IN/
% locale mashtape da-DK %locale/bn-IN/
% locale mashtape de-DE %locale/bn-IN/
% skin mashtape classic/1.0 %skin/
% overlay chrome://browser/content/preferences/preferences.xul chrome://mashtape/content/prefsOverlay.xul
% style chrome://songbird/content/xul/layoutBaseOverlay.xul chrome://mashtape/skin/mainwin.css
@include_all_to
