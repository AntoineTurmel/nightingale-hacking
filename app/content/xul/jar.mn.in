songbird.jar:
#ifdef USING_FLAT_JARS
% content  songbird  songbird/content/songbird/
#else
% content  songbird  jar:songbird.jar!/content/songbird/
#endif
% overlay chrome://browser/content/preferences/preferences.xul               chrome://songbird/content/xul/preferences/main_overlay.xul
% overlay chrome://browser/content/preferences/permissions.xul               chrome://songbird/content/xul/preferences/permissions_overlay.xul
% overlay chrome://mozapps/content/extensions/extensions.xul                 chrome://songbird/content/xul/extensionManagerOverlay.xul
% overlay chrome://mozapps/content/extensions/extensions.xul?type=extensions chrome://songbird/content/xul/extensionManagerOverlay.xul
% overlay chrome://mozapps/content/extensions/extensions.xul?type=themes     chrome://songbird/content/xul/extensionManagerOverlay.xul
% overlay chrome://mozapps/content/update/updates.xul                        chrome://songbird/content/xul/updateOverlay.xul
@include_all_to content/songbird/xul/
#ifdef MEDIA_CORE_VLC
% overlay chrome://songbird/content/xul/songbird.xul                         chrome://songbird/content/xul/coreVLCOverlay.xul
#endif
#ifdef MEDIA_CORE_GST
% overlay chrome://songbird/content/xul/songbird.xul                         chrome://songbird/content/xul/coreGStreamerOverlay.xul
#endif
