nightingale.jar:
% content  nightingale  %content/nightingale/
# preferences overlays
* content/nightingale/xul/preferences/mainOverlay.xul                           (preferences/mainOverlay.xul)
% overlay chrome://browser/content/preferences/preferences.xul               chrome://nightingale/content/xul/preferences/mainOverlay.xul
% overlay chrome://browser/content/preferences/tabs.xul                      chrome://nightingale/content/xul/preferences/tabsOverlay.xul
% overlay chrome://browser/content/preferences/permissions.xul               chrome://nightingale/content/xul/preferences/permissionsOverlay.xul
% overlay chrome://browser/content/preferences/sanitize.xul                  chrome://nightingale/content/xul/preferences/sanitizeOverlay.xul
% overlay chrome://browser/content/preferences/advanced.xul                  chrome://nightingale/content/xul/preferences/advancedOverlay.xul
# remove help button from pref windows
% overlay chrome://browser/content/preferences/colors.xul                    chrome://nightingale/content/xul/preferences/helpRemovalOverlay.xul
% overlay chrome://browser/content/preferences/connection.xul                chrome://nightingale/content/xul/preferences/helpRemovalOverlay.xul
% overlay chrome://browser/content/preferences/fonts.xul                     chrome://nightingale/content/xul/preferences/helpRemovalOverlay.xul
% overlay chrome://browser/content/preferences/languages.xul                 chrome://nightingale/content/xul/preferences/helpRemovalOverlay.xul
# EM overlays
% overlay chrome://mozapps/content/extensions/extensions.xul                 chrome://nightingale/content/xul/extensionManagerOverlay.xul
% overlay chrome://mozapps/content/extensions/extensions.xul?type=extensions chrome://nightingale/content/xul/extensionManagerOverlay.xul
% overlay chrome://mozapps/content/extensions/extensions.xul?type=themes     chrome://nightingale/content/xul/extensionManagerOverlay.xul
% overlay chrome://mozapps/content/update/updates.xul                        chrome://nightingale/content/xul/updateOverlay.xul
% overlay chrome://browser/content/sanitize.xul                              chrome://nightingale/content/xul/preferences/sanitizeOverlay.xul
% overlay chrome://mozapps/content/downloads/unknownContentType.xul          chrome://nightingale/content/xul/unknownContentTypeOverlay.xul
@include_all_to content/nightingale/xul/
* content/nightingale/xul/trackEditor.xul                                       (trackEditor.xul)
* content/nightingale/xul/trackEditorVideo.xul                                  (trackEditorVideo.xul)
* content/nightingale/xul/layoutWithBrowserOverlay.xul                          (layoutWithBrowserOverlay.xul)
% override chrome://browser/content/browser.xul                                       chrome://nightingale/content/xul/browserCompatibilityStub.xul
% overlay chrome://nightingale/content/xul/layoutBaseOverlay.xul                chrome://nightingale/content/xul/linuxMenuOverlay.xul OS=Linux
% overlay chrome://nightingale/content/xul/layoutBaseOverlay.xul                chrome://nightingale/content/xul/macMenuOverlay.xul OS=Darwin
% overlay chrome://nightingale/content/xul/layoutWithBrowserOverlay.xul         chrome://nightingale/content/xul/contentAreaContextOverlay.xul
# Profile overlays
% overlay chrome://mozapps/content/profile/profileSelection.xul              chrome://nightingale/content/xul/profileSelectionOverlay.xul
