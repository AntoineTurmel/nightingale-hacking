#ifndef __sbBuildIDs_h__
#define __sbBuildIDs_h__

/**
 * This identifies the app that is being created
 */
#define SB_APPNAME Songbird

/**
 * This identifies the name of the current source branch
 */
#define SB_BRANCHNAME trunk

/**
 * This uniquely identifies the build (format: YYYYMMDDhhmmss). It will only be
 * set if SONGBIRD_OFFICIAL is defined.
 */ 
#define SB_BUILD_ID 00000000000000

/**
 * These identify the target milestone
 */
#define SB_MILESTONE 0.3

/**
 * This identifies the mozilla.org tag used to build XULRunner
 *
 * XXXredfive - unused, do we actually need this?
 */
#define SB_MOZILLA_TAG SONGBIRD_0_2A_20060518_BRANCH

#endif /* __sbBuildIDs_h__ */
