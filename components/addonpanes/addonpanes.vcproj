<?xml version="1.0" encoding="Windows-1252"?>
<VisualStudioProject
	ProjectType="Visual C++"
	Version="7.10"
	Name="addonpanes"
	ProjectGUID="{8DD67C9F-32BA-47FC-AB04-EFFCE0DF1503}"
	RootNamespace="addonpanes"
	Keyword="Win32Proj">
	<Platforms>
		<Platform
			Name="Win32"/>
	</Platforms>
	<Configurations>
		<Configuration
			Name="Debug|Win32"
			OutputDirectory="obj\$(ConfigurationName)"
			IntermediateDirectory="obj\$(ConfigurationName)"
			ConfigurationType="10"
			CharacterSet="1"
			DeleteExtensionsOnClean="*.obj;*.ilk;*.pdb;*.tlb;*.tli;*.tlh;*.tmp;*.rsp;*.bat;$(OutDir)\BuildLog.htm;$(TargetPath)">
			<Tool
				Name="VCCustomBuildTool"/>
			<Tool
				Name="VCMIDLTool"/>
			<Tool
				Name="VCPostBuildEventTool"
				CommandLine="mkdir $(SolutionDir)dist\components
copy /y $(InputDir)src\sbAddOnPanes.js $(SolutionDir)dist\components
copy /y $(InputDir)$(OutDir)\sbIAddOnPanes.xpt $(SolutionDir)dist\components
echo. &gt; $(SolutionDir)dist\xulrunner\.autoreg
"/>
			<Tool
				Name="VCPreBuildEventTool"/>
		</Configuration>
		<Configuration
			Name="Release|Win32"
			OutputDirectory="obj\$(ConfigurationName)"
			IntermediateDirectory="obj\$(ConfigurationName)"
			ConfigurationType="10"
			CharacterSet="1"
			DeleteExtensionsOnClean="*.obj;*.ilk;*.pdb;*.tlb;*.tli;*.tlh;*.tmp;*.rsp;*.bat;*.htm;$(TargetPath)">
			<Tool
				Name="VCCustomBuildTool"/>
			<Tool
				Name="VCMIDLTool"/>
			<Tool
				Name="VCPostBuildEventTool"
				CommandLine="mkdir $(SolutionDir)dist\components
copy /y $(InputDir)src\sbAddOnPanes.js $(SolutionDir)dist\components
copy /y $(InputDir)$(OutDir)\sbIAddOnPanes.xpt $(SolutionDir)dist\components
echo. &gt; $(SolutionDir)dist\xulrunner\.autoreg
"/>
			<Tool
				Name="VCPreBuildEventTool"/>
		</Configuration>
	</Configurations>
	<References>
	</References>
	<Files>
		<Filter
			Name="public"
			Filter="">
			<File
				RelativePath=".\public\Makefile.in">
			</File>
			<File
				RelativePath=".\public\sbIAddOnPanes.idl">
				<FileConfiguration
					Name="Debug|Win32">
					<Tool
						Name="VCCustomBuildTool"
						Description="XPIDL: $(InputFileName) -&gt; $(InputName).h and $(InputName).xpt"
						CommandLine="$(SolutionDir)dependencies\win32\mozilla\$(ConfigurationName)\bin\xpidl -m header -I $(SolutionDir)dependencies\win32\mozilla\$(ConfigurationName)\idl -I $(InputDir) -o $(OutDir)\$(InputName) $(InputPath)
$(SolutionDir)dependencies\win32\mozilla\$(ConfigurationName)\bin\xpidl -m typelib -I $(SolutionDir)dependencies\win32\mozilla\$(ConfigurationName)\idl -I $(InputDir) -o $(OutDir)\$(InputName) $(InputPath)
"
						Outputs="$(OutDir)\$(InputName).h;$(OutDir)\$(InputName).xpt"/>
				</FileConfiguration>
				<FileConfiguration
					Name="Release|Win32">
					<Tool
						Name="VCCustomBuildTool"
						Description="XPIDL: $(InputFileName) -&gt; $(InputName).h and $(InputName).xpt"
						CommandLine="$(SolutionDir)dependencies\win32\mozilla\$(ConfigurationName)\bin\xpidl -m header -I $(SolutionDir)dependencies\win32\mozilla\$(ConfigurationName)\idl -I $(InputDir) -o $(OutDir)\$(InputName) $(InputPath)
$(SolutionDir)dependencies\win32\mozilla\$(ConfigurationName)\bin\xpidl -m typelib -I $(SolutionDir)dependencies\win32\mozilla\$(ConfigurationName)\idl -I $(InputDir) -o $(OutDir)\$(InputName) $(InputPath)
"
						Outputs="$(OutDir)\$(InputName).h;$(OutDir)\$(InputName).xpt"/>
				</FileConfiguration>
			</File>
		</Filter>
		<Filter
			Name="src"
			Filter="">
			<File
				RelativePath=".\src\Makefile.in">
			</File>
			<File
				RelativePath=".\src\sbAddOnPanes.js">
			</File>
		</Filter>
		<File
			RelativePath=".\Makefile.in">
		</File>
	</Files>
	<Globals>
	</Globals>
</VisualStudioProject>
