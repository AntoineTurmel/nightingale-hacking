<profile>
  <type>video</type>
  <id>fa25e632-1dd1-11b2-a48f-c58a35593c6d</id>
  <priority>1000</priority> <!-- Low priority; this is not optimized -->
  <priority quality="0">2000</priority> 
  <priority quality="1">2000</priority>
  <description>MP4/MJPEG/ADPCM test profile</description>
  <mimetype>video/quicktime</mimetype>
  <extension>mp4</extension>

  <container>
    <type>video/quicktime</type>
  </container>

  <video>
    <type>image/jpeg</type>
    <quality-property name="bpp" quality="0" value="1.5"/>
    <quality-property name="bpp" quality="1" value="12"/>
    <property name="quality" type="int" min="0" max="100" default="100"
              mapping="quality" scale="100"/>
  </video>

  <audio>
    <type>audio/x-adpcm</type>
    <!-- the bitrate is constant and not tweakable -->
    <quality-property name="bitrate" quality="0.5" value="200000"/>
  </audio>
</profile>
