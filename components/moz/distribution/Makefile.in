DEPTH = ../../..
topsrcdir = @top_srcdir@
srcdir = @srcdir@
VPATH = @srcdir@

include $(DEPTH)/build/autodefs.mk

# don't use SONGBIRD_JSMODULES because we need to rename the file too

libs:: $(SONGBIRD_JSMODULESDIR)/Distribution.jsm

$(SONGBIRD_JSMODULESDIR)/Distribution.jsm: $(topsrcdir)/dependencies/vendor/mozbrowser/components/distribution.js
ifeq (,$(wildcard $(SONGBIRD_JSMODULESDIR)))
	$(MKDIR) -p $(SONGBIRD_JSMODULESDIR)
endif
	$(INSTALL_FILE) $< $@

include $(topsrcdir)/build/rules.mk

