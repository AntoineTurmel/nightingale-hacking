        <type>PRInt32</type>
        <definition>PRInt32 sbITranscodeJob::vote</definition>
        <argsstring>(in sbIMediaItem aMediaItem, in sbITranscodeProfile aProfile)</argsstring>
        <name>vote</name>
        <param>
          <type>in <ref refid="interfacesb_i_media_item" kindref="compound">sbIMediaItem</ref></type>
          <declname>aMediaItem</declname>
        </param>
        <param>
